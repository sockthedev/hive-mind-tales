{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "pnpm run generate:css --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "pnpm run generate:css --watch",
    "dev:remix": "remix dev",
    "generate:css": "pnpm exec tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/serve": "^1.12.0",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.12.0",
    "@remix-run/eslint-config": "^1.12.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@remix-run/eslint-config",
      "@remix-run/eslint-config/node"
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  }
}
